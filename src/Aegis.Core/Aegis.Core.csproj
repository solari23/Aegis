<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Authors>Alexandre Kerametlian</Authors>
    <Description>Core library providing Aegis secure file storage functionality.</Description>
    <RepositoryUrl>https://github.com/solari23/Aegis</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup>
    <BondOptions>--readonly-properties --using="DateTime=System.DateTime" --using="Guid=System.Guid"</BondOptions>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bond.Compiler.CSharp" Version="8.0.0" />
    <PackageReference Include="Bond.Core.CSharp" Version="8.0.0" />
  </ItemGroup>

</Project>
